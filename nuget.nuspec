    <file src="js/searchbuilder.bootstrap4.min.js" target="contentFiles\any\any\wwwroot\js"/>
    <file src="css/searchbuilder.bootstrap4.css" target="contentFiles\any\any\wwwroot\css"/>
    <file src="css/searchbuilder.bootstrap4.min.css" target="contentFiles\any\any\wwwroot\css"/>
