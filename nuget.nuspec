<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>datatables.net-searchbuilder-bs4</id>
    <version>1.0.0</version>
    <description>SearchBuilder for DataTables</description>
    <authors>SpryMedia Ltd</authors>
    <projectUrl>http://datatables.net</projectUrl>
    <license type="expression">MIT</license>
    <tags>searchbuilder DataTables jQuery table Bootstrap Bootstrap4</tags>
    <repository type="git" url="https://github.com/DataTables/Dist-DataTables-SearchBuilder-Bootstrap4.git"/>
    <dependencies>
